          <v-icon> mdi-delete </v-icon>\n        </v-btn>\n      </v-card>\n    </v-col>\n\n    <v-dialog v-model=\"dialog\" persistent max-width=\"600px\">\n      <v-card>\n        <v-card-title>\n          <span class=\"headline\">Editar</span>\n        </v-card-title>\n        <v-card-text>\n          <v-container>\n            <v-row>\n              <v-col cols=\"12\">\n                <v-textarea\n                  v-model=\"message\"\n                  label=\"Message\"\n                  type=\"password\"\n                  required\n                ></v-textarea>\n              </v-col>\n              <v-col cols=\"12\" sm=\"6\"> </v-col>\n            </v-row>\n          </v-container>\n        </v-card-text>\n        <v-card-actions>\n          <v-spacer></v-spacer>\n          <v-btn\n            text\n            @click=\"dialog = false\"\n            color=\"red\"\n            class=\"ml-4 mr-2 mb-2\"\n            outlined\n          >\n            FECHAR\n          </v-btn>\n\n          <v-btn\n            color=\"blue\"\n            class=\"ml-4 mr-2 mb-2\"\n            outlined\n            @click=\"atualizar(id, message)\"\n          >\n            Editar\n          </v-btn>\n        </v-card-actions>\n      </v-card>\n    </v-dialog>\n  </v-row>\n</template>\n\n<script>\nexport default {\n  name: \"Card\",\n  props: {\n    dados: {\n      default: () => {},\n    },\n  },\n  data: () => ({\n    id: \"\",\n    message: \"\",\n    dialog: false,\n  }),\n\n  methods: {\n    excluir(id) {\n      this.$emit(\"apagarMessage\", id);\n    },\n\n    atualizar(id, message) {\n      this.$emit(\"atualizarMessage\", { id, message });\n    },\n  },\n};\n</script>\n\n<style>\n</style>"]}]}